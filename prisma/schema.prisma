datasource db {
  provider = "postgresql"
  url      = "postgresql://postgres:docker@192.168.99.100:5432/pg-docker?schema=public"
}

generator client {
  provider = "prisma-client-js"
}

model Vehicle {
  id        String   @default(cuid()) @id
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  make      String
  image     String
  model     String
  year      String
  price     String
  power     String
  torque    String
  engine    String
  sixty     String
  topSpeed  String
  weight    String
  vtype     String
}